{
  "name": "gulp-coffee-browserify",
  "version": "1.0.0",
  "description": "using gulp-coffee with browserify",
  "main": {
    "es6": "src/main.es6",
    "coffee": "src/main.coffee"
  },
  "local-web-server": {
    "port": 3000,
    "forbid": [
      "*.json",
      "*.ts",
      "*.md",
      "*.yml",
      "*.babel.js",
      ".gitignore"
    ],
    "no-cache": true
  },
  "scripts": {
    
    "gulp": "gulp",
    "pregulp": "npm i",
    "rm": "rm -fr dist",
    "test": "npm run gulp",
    "prestart": "npm test",
    "start": "gulp watch & ws"
  },
  "author": "Maksim Kostromin",
  "license": "ISC",
  "devDependencies": {
    "bootstrap": "^3.3.6",
    "browserify": "^13.0.0",
    "coffee-script": "^1.10.0",
    "coffeeify": "^2.0.1",
    "colors": "^1.1.2",
    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.0",
    "gulp-cssnano": "^2.1.1",
    "gulp-livereload": "^3.8.1",
    "local-web-server": "^1.1.1",
    "jquery": "^2.2.1",
    "require-dir": "^0.3.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daggerok/gulp-coffee-browserify.git"
  },
  "keywords": [
    "gulp-coffee",
    "coffee-script",
    "browserify",
    "coffeeify",
    "jquery",
    "bootstrap",
    "local-web-server",
    "gulp-livereload"
  ],
  "bugs": {
    "url": "https://github.com/daggerok/gulp-coffee-browserify/issues"
  },
  "homepage": "https://github.com/daggerok/gulp-coffee-browserify#readme"
}